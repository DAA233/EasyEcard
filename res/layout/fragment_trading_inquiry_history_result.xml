<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/history2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <com.duang.easyecard.UI.PinnedHeaderExpandableListView
        android:id="@+id/histroy_trading_inquiry_expandablelist"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="11"
        android:cacheColorHint="@null"
        android:childDivider="@drawable/trading_inquiry_child_bg"
        android:childIndicatorLeft="0dp"
        android:divider="@color/grey"
        android:dividerHeight="1dp"
        android:groupIndicator="@null"
        android:scrollbarAlwaysDrawHorizontalTrack="false" >
    </com.duang.easyecard.UI.PinnedHeaderExpandableListView>
    
    <LinearLayout 
        android:id="@+id/history_trading_inquiry_foot_linear"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:background="@color/black_tab">
       
        <TextView 
            android:id="@+id/history_trading_inquiry_result_start_time"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="13sp"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:background="@color/dark"/>
        <TextView 
            android:id="@+id/history_trading_inquiry_result_end_time"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="13sp"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:background="@color/dark"/>
        <TextView 
            android:id="@+id/history_trading_inquiry_result_foot_output"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="13sp"
            android:text="@string/history_trading_inquiry_result_foot_output"/>
    </LinearLayout>

</LinearLayout>
