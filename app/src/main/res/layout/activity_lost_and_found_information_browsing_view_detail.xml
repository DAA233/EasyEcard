<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context="com.duang.easyecard.Activity.LostAndFoundRegistrationActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <br.com.dina.ui.widget.UITableView
            android:id="@+id/lost_and_found_information_browsing_view_detail_table_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.rey.material.widget.Button
            android:id="@+id/lost_and_found_fragment_information_browsing_view_detail_button"
            style="@style/button_ripple_wave"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="40dp"
            android:onClick="onButtonClick" />
    </LinearLayout>
</ScrollView>
