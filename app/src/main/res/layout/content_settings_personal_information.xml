<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.duang.easyecard.Activity.SettingsPersonalInformationActivity"
    tools:showIn="@layout/activity_settings_personal_information">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.rey.material.widget.ProgressView
            android:id="@+id/settings_personal_information_progress_view"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="80dp"
            app:pv_autostart="true"
            app:pv_progressMode="indeterminate" />

        <br.com.dina.ui.widget.UITableView
            android:id="@+id/settings_personal_information_table_view"
            style="@style/UITableView" />
    </FrameLayout>

</ScrollView>
